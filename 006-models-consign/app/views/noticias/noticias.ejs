<!DOCTYPE html>
	<html lang="pt-br">
		<head>
			<meta charset="utf-8"/>
			<title>Notícias</title>
		</head>
	<body>

		<h1>Notícias Home</h1>

		<table>
				<tr>
					<td>ID</td>
					<td>Título</td>
					<td>Notícia</td>
				</tr>
				<% for(let i = 0; i < noticias.length; i++) { %>
					<tr>
						<td><%= noticias[i].id %></td>
						<td><%= noticias[i].titulo %></td>
						<td><%= noticias[i].noticia %></td>
					</tr>
				<% } %>
			</table>
	</body>
</html>
